<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kbb12.dms">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StartUp.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Template.TemplateActivity"
            android:label="@string/title_activity_template"
            android:parentActivityName=".StartUp.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.kbb12.dms.StartUp.MainActivity" />
        </activity>
        <activity android:name=".MealList.MealListActivity" />
        <activity android:name=".AddIngredient.AddIngredientActivity" />
        <activity android:name=".CustomIngredient.AddCustomIngredientActivity" />
        <!--
 ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity android:name=".IngredientList.IngredientListActivity" />
        <activity android:name=".IngredientAmount.IngredientsAmountActivity" />
        <activity android:name=".MealAmount.MealAmountActivity" />
        <activity android:name=".MealCarbohydrateValue.MealCarbohydrateValueActivity"></activity>
    </application>

</manifest>